{
	"name": "narsil/localization",
	"description": "Narsil localization",
	"license": "MIT",
	"keywords": [
		"narsil",
		"laravel",
		"localization",
		"translations"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^11.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Narsil\\Localization\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Narsil\\Localization\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Narsil\\Localization\\NarsilLocalizationServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	}
}
