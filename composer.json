{
	"name": "narsil/laravel-translations",
	"description": "Laravel translations",
	"license": "MIT",
	"keywords": [
		"narsil",
		"laravel",
		"translation",
		"translations"
	],
	"authors": [
		{
			"name": "Jonathan Rigaux"
		}
	],
	"require": {
		"php": "^8.2",
		"laravel/framework": "^11.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Narsil\\Translations\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Narsil\\Translations\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Narsil\\Translations\\TranslationsServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	}
}
